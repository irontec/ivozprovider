{
    "name": "irontec/ivozprovider-shared-dependencies",
    "description": "Multitenant solution for VoIP telephony providers (Shared dependencies)",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Gaizka Elexpe",
            "email": "gaizka@irontec.com"
        },
        {
            "name": "Gorka Gorrotxategi",
            "email": "gorka.g@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Luis Felipe GarcÃ­a",
            "email": "lfgarcia@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        },
        {
            "name": "Victor Vargas",
            "email": "victor@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./composer-packages/*/*",
            "options": {
                "symlink": false
            }
        },
        {
            "url": "https://github.com/irontec/wkhtmltopdf-amd64.git",
            "type": "git"
        },
        {
            "url": "https://github.com/irontec/zend-mp3.git",
            "type": "git"
        },
        {
            "url": "https://github.com/irontec/prophecy.git",
            "type": "git"
        },
        {
            "url": "https://github.com/irontec/handlebars.php",
            "type": "git"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/",
            "Ivoz\\": "Ivoz"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        },
        "platform": {
            "php": "8.0.9"
        },
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "require": {
        "php": ">=8.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "doctrine/persistence": "^2.1",
        "irontec/ivoz-api-bundle": "^4.7",
        "irontec/ivoz-core": "^4.18.0",
        "irontec/ivoz-dev-tools": "^5.12.1",
        "irontec/ivoz-provider-bundle": "^2.6.7",
        "irontec/prophecy": "^1.13",
        "irontec/replacements": "^1.0",
        "irontec/wkhtmltopdf-amd64": "0.12.6.1",
        "irontec/zend-mp3": "^1.8",
        "knplabs/knp-snappy": "^1.2",
        "ocramius/package-versions": "^1.9",
        "php-mime-mail-parser/php-mime-mail-parser": "^7.0",
        "phpxmlrpc/phpxmlrpc": "^4.0",
        "snc/redis-bundle": "^3.2",
        "symfony/polyfill-util": "^1.22",
        "xamin/handlebars.php": "v0.10.5"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "behat/behat": "^3.1",
        "behat/mink-browserkit-driver": "^2.0",
        "behat/mink-extension": "^2.2",
        "behatch/contexts": "^3.3",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "http-interop/http-middleware": "^0.5",
        "overtrue/phplint": "^1.0",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan": "^0.12.81",
        "phpstan/phpstan-beberlei-assert": "^0.12.6",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.21",
        "phpunit/phpunit": "^9.5.3",
        "rector/rector": "^0.11.57",
        "squizlabs/php_codesniffer": "^3.6.2",
        "swoole/ide-helper": "^4.3.4",
        "symfony/phpunit-bridge": "^5.1",
        "vimeo/psalm": "^4.0"
    }
}
