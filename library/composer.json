    "repositories": [
        {
            "type": "path",
            "url": "./composer-packages/*/*",
            "options": {
                "symlink": false
            }
        },
        {
            "url": "https://github.com/mmadariaga/TestDoubleBundle.git",
            "type": "git"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/",
            "Ivoz\\": "Ivoz"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        },
        "platform": {
            "php": "7.0.19"
        }
    },
    "require": {
        "php": ">=7.0.19",
        "debach/zend-mp3": "1.8.*",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "h4cc/wkhtmltopdf-amd64": "0.12.3",
        "irontec/ivoz-provider-bundle": "^2.0",
        "irontec/ivoz-api-bundle": "^3.1",
        "irontec/ivoz-dev-tools": "^3.0",
        "knplabs/knp-snappy": "0.4.3",
        "mmoreram/gearman-bundle": "^4.1",
        "php-mime-mail-parser/php-mime-mail-parser": "^2.9",
        "phpxmlrpc/phpxmlrpc": "^4.0",
        "snc/redis-bundle": "2.1.*",
        "xamin/handlebars.php": "^0.10.4"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3.1",
