<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Provider\Domain\Model\DdiProviderRegistration\DdiProviderRegistrationAbstract" table="ddi_provider_registration_abstract">
    <unique-constraints>
      <unique-constraint name="ddiProviderRegistration_username_domain" columns="username,domain"/>
    </unique-constraints>
    <field name="username" type="string" column="username" length="64" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="domain" type="string" column="domain" length="190" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="realm" type="string" column="realm" length="64" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="authUsername" type="string" column="authUsername" length="64" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="authPassword" type="string" column="authPassword" length="64" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="authProxy" type="string" column="authProxy" length="64" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="expires" type="integer" column="expires" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="multiDdi" type="boolean" column="multiDdi" nullable="true">
      <options>
        <option name="default">0</option>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="contactUsername" type="string" column="contactUsername" length="64" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <one-to-one field="trunksUacreg" target-entity="Ivoz\Kam\Domain\Model\TrunksUacreg\TrunksUacreg" mapped-by="ddiProviderRegistration" fetch="LAZY"/>
    <many-to-one field="ddiProvider" target-entity="Ivoz\Provider\Domain\Model\DdiProvider\DdiProvider" inversed-by="ddiProviderRegistrations" fetch="LAZY">
      <join-columns>
        <join-column name="ddiProviderId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
