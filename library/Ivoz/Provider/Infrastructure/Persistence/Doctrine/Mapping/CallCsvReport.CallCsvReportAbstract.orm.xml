<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Provider\Domain\Model\CallCsvReport\CallCsvReportAbstract" table="call_csv_report_abstract">
    <embedded name="csv" class="Ivoz\Provider\Domain\Model\CallCsvReport\Csv" use-column-prefix="false" />
    <field name="sentTo" type="string" column="sentTo" length="250" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="inDate" type="datetime" column="inDate" nullable="false"/>
    <field name="outDate" type="datetime" column="outDate" nullable="false"/>
    <field name="createdOn" type="datetime" column="createdOn" nullable="false"/>
    <many-to-one field="company" target-entity="Ivoz\Provider\Domain\Model\Company\Company" fetch="LAZY">
      <join-columns>
        <join-column name="companyId" referenced-column-name="id" on-delete="SET NULL" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="brand" target-entity="Ivoz\Provider\Domain\Model\Brand\Brand" fetch="LAZY">
      <join-columns>
        <join-column name="brandId" referenced-column-name="id" on-delete="SET NULL" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="callCsvScheduler" target-entity="Ivoz\Provider\Domain\Model\CallCsvScheduler\CallCsvScheduler" fetch="LAZY">
      <join-columns>
        <join-column name="callCsvSchedulerId" referenced-column-name="id" on-delete="SET NULL" nullable="1"/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
