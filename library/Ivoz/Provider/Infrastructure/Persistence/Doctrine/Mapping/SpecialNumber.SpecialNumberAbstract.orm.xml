<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Provider\Domain\Model\SpecialNumber\SpecialNumberAbstract" table="special_number_abstract">
    <field name="number" type="string" column="number" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numberE164" type="string" column="numberE164" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="disableCDR" type="integer" column="disableCDR" nullable="false">
      <options>
        <option name="default">1</option>
        <option name="unsigned">1</option>
      </options>
    </field>
    <many-to-one field="brand" target-entity="Ivoz\Provider\Domain\Model\Brand\Brand" fetch="LAZY">
      <join-columns>
        <join-column name="brandId" referenced-column-name="id" on-delete="CASCADE" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="country" target-entity="Ivoz\Provider\Domain\Model\Country\Country" fetch="LAZY">
      <join-columns>
        <join-column name="countryId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
