<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Provider\Domain\Model\FeaturesRelCompany\FeaturesRelCompanyAbstract" table="features_rel_company_abstract">
    <unique-constraints>
      <unique-constraint name="featureRelCompany_feature_brand" columns="featureId,companyId"/>
    </unique-constraints>
    <many-to-one field="company" target-entity="Ivoz\Provider\Domain\Model\Company\Company" inversed-by="relFeatures" fetch="LAZY">
      <join-columns>
        <join-column name="companyId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
    <many-to-one field="feature" target-entity="Ivoz\Provider\Domain\Model\Feature\Feature" fetch="LAZY">
      <join-columns>
        <join-column name="featureId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
