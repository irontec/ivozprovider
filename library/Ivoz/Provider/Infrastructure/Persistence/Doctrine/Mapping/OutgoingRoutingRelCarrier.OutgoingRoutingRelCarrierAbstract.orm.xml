<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Provider\Domain\Model\OutgoingRoutingRelCarrier\OutgoingRoutingRelCarrierAbstract" table="outgoing_routing_rel_carrier_abstract">
    <unique-constraints>
      <unique-constraint name="outgoingRoutingRelCarrier_carrier" columns="outgoingRoutingId,carrierId"/>
    </unique-constraints>
    <many-to-one field="outgoingRouting" target-entity="Ivoz\Provider\Domain\Model\OutgoingRouting\OutgoingRouting" inversed-by="relCarriers" fetch="LAZY">
      <join-columns>
        <join-column name="outgoingRoutingId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
    <many-to-one field="carrier" target-entity="Ivoz\Provider\Domain\Model\Carrier\Carrier" inversed-by="outgoingRoutingsRelCarriers" fetch="LAZY">
      <join-columns>
        <join-column name="carrierId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
