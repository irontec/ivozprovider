<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Ivoz\Provider\Domain\Model\ExternalCallFilter\ExternalCallFilterAbstract" table="external_call_filter_abstract">
    <unique-constraints>
      <unique-constraint name="externalCallFilter_name_company" columns="name,companyId"/>
    </unique-constraints>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="holidayTargetType" type="string" column="holidayTargetType" length="25" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">[enum:number|extension|voicemail]</option>
      </options>
    </field>
    <field name="holidayNumberValue" type="string" column="holidayNumberValue" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="outOfScheduleTargetType" type="string" column="outOfScheduleTargetType" length="25" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">[enum:number|extension|voicemail]</option>
      </options>
    </field>
    <field name="outOfScheduleNumberValue" type="string" column="outOfScheduleNumberValue" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="company" target-entity="Ivoz\Provider\Domain\Model\Company\Company" fetch="LAZY">
      <join-columns>
        <join-column name="companyId" referenced-column-name="id" on-delete="CASCADE" nullable=""/>
      </join-columns>
    </many-to-one>
    <many-to-one field="welcomeLocution" target-entity="Ivoz\Provider\Domain\Model\Locution\Locution" fetch="LAZY">
      <join-columns>
        <join-column name="welcomeLocutionId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="holidayLocution" target-entity="Ivoz\Provider\Domain\Model\Locution\Locution" fetch="LAZY">
      <join-columns>
        <join-column name="holidayLocutionId" referenced-column-name="id" on-delete="SET NULL" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="outOfScheduleLocution" target-entity="Ivoz\Provider\Domain\Model\Locution\Locution" fetch="LAZY">
      <join-columns>
        <join-column name="outOfScheduleLocutionId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="holidayExtension" target-entity="Ivoz\Provider\Domain\Model\Extension\Extension" fetch="LAZY">
      <join-columns>
        <join-column name="holidayExtensionId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="outOfScheduleExtension" target-entity="Ivoz\Provider\Domain\Model\Extension\Extension" fetch="LAZY">
      <join-columns>
        <join-column name="outOfScheduleExtensionId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="holidayVoiceMailUser" target-entity="Ivoz\Provider\Domain\Model\User\User" fetch="LAZY">
      <join-columns>
        <join-column name="holidayVoiceMailUserId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="outOfScheduleVoiceMailUser" target-entity="Ivoz\Provider\Domain\Model\User\User" fetch="LAZY">
      <join-columns>
        <join-column name="outOfScheduleVoiceMailUserId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="holidayNumberCountry" target-entity="Ivoz\Provider\Domain\Model\Country\Country" fetch="LAZY">
      <join-columns>
        <join-column name="holidayNumberCountryId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="outOfScheduleNumberCountry" target-entity="Ivoz\Provider\Domain\Model\Country\Country" fetch="LAZY">
      <join-columns>
        <join-column name="outOfScheduleNumberCountryId" referenced-column-name="id" on-delete="SET NULL"/>
      </join-columns>
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
