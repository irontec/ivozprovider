{
    "name": "irontec/ivozprovider/rest-api",
    "description": "Multitenant solution for VoIP telephony providers (Rest-API)",
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../library/vendor/*/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "IvozProvider\\": "../../library/IvozProvider"
        },
        "psr-4": {
            "": "src/",
            "Ivoz\\": "../../library/Ivoz",
            "DataFixtures\\": "../../library/DataFixtures",
            "CoreBundle\\": "../../library/CoreBundle",
            "ApiBundle\\": "../../library/ApiBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ],
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        },
        "platform": {
            "php": "7.0.19"
        }
    },
    "require": {
        "php": ">=7.0.19",
        "api-platform/core": "2.1.*",
        "beberlei/assert": "2.9.*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "ramsey/uuid": "^3.7",
        "sensio/distribution-bundle": "5.0.*",
        "symfony/monolog-bundle": "3.1.*",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0 || ^2.0",
        "knplabs/knp-snappy": "0.4.3",
        "xamin/handlebars.php": "^0.10.4",
        "h4cc/wkhtmltopdf-amd64": "0.12.3",
        "h4cc/wkhtmltopdf-i386": "0.12.3",
        "graze/guzzle-jsonrpc": "3.2.*"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "api-platform/schema-generator": "^1.0 || ^2.0",
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3.1",
        "behat/mink-extension": "^2.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^2.5",
        "docteurklein/test-double-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "http-interop/http-middleware": "^0.5",
        "mkraemer/react-pcntl": "^2.0|^3.0",
        "paragonie/random_compat": "^2.0",
        "php-pm/httpkernel-adapter": "^1.0.1",
        "php-pm/php-pm": "^1.0.0",
        "phpunit/phpunit": "^5.3",
        "react/child-process": "^0.5",
        "react/event-loop": "^0.4",
        "react/http": "^0.8",
        "react/socket": "^0.8.6",
        "react/stream": "^0.7.1",
        "ringcentral/psr7": "^1.2",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "../../library/CoreBundle/Resources/config/parameters.yml"
        },
        "branch-alias": null
    }
}
