{
    "name": "ivozprovider/rest-api",
    "description": "Multitenant solution for VoIP telephony providers (Rest-API)",
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../../library/vendor/*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "url": "https://github.com/symfony/polyfill-php56.git",
            "type": "git"
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "IvozProvider\\": "../../../library/IvozProvider"
        },
        "psr-4": {
            "": "src/",
            "Ivoz\\": "../../../library/Ivoz",
            "DataFixtures\\": "../../../library/DataFixtures"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        }
    },
    "require": {
        "irontec/ivoz-api-bundle": "^4.0",
        "irontec/ivoz-provider-bundle": "^2.0",
        "irontec/replacements": "^1.0",
        "knplabs/knp-snappy": "^1.2",
        "symfony/flex": "^1.9",
        "twig/twig": "^3.0",
        "xamin/handlebars.php": "^0.10.4"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "friends-of-behat/symfony-extension": "^2.0",
        "behatch/contexts": "^3.3",
        "docteurklein/test-double-bundle": "^1.0",
        "irontec/prophecy": "^1.13",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpunit/phpunit": "^9.4.4",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "symfony/phpunit-bridge": "^5.0",
        "vimeo/psalm": "^4.0"
    },
    "extra": {
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "vendor/irontec/ivoz-provider-bundle/Resources/config/parameters.yml"
        },
        "branch-alias": null
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
